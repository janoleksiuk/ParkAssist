<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/nazwaParkinguKNL"
        tools:text="Nazwa parkingu"
        />

    <TextView
        android:id="@+id/polozenieParkinguKNL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nazwaParkinguKNL"
        tools:text="Polozenie na mapie" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@+id/polozenieParkinguKNL"
        android:layout_marginTop="9dp"
        android:background="@color/black" />
</RelativeLayout>